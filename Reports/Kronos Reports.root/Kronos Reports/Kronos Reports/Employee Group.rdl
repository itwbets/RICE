  <DataSets>
    <DataSet Name="M_stp_itw_rpt_EmployeeGroup">
      <Fields>
        <Field Name="SHORTNM">
          <DataField>SHORTNM</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="HOMELABORLEVELNM1">
          <DataField>HOMELABORLEVELNM1</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ISWFCMGRLICENCE">
          <DataField>ISWFCMGRLICENCE</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="personfullname">
          <DataField>PERSONFULLNAME</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>Kronos</DataSourceName>
        <CommandText>SELECT     a.SHORTNM, b.HOMELABORLEVELNM1, b.ISWFCMGRLICENCE, 
                      b.PERSONFULLNAME
FROM         dbo.LABORACCTSET a inner JOIN
                      dbo.VP_PERSONV42 b on a.shortnm = b.mgraccesslaborset
where laboracctsetid in ( @pLabour_Account_Set )
and iswfcmgrlicence in (@pManager_Licence)</CommandText>
        <QueryParameters>
          <QueryParameter Name="@pLabour_Account_Set">
            <Value>=Parameters!pLabour_Account_Set.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
          <QueryParameter Name="@pManager_Licence">
            <Value>=Parameters!pManager_Licence.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Filters>
        <Filter>
          <FilterExpression>=IIf(Parameters!pLabour_Account_Set_Count.Value = Parameters!pLabour_Account_Set.Count,Left(Fields!HOMELABORLEVELNM1.Value,2),1)</FilterExpression>
          <Operator>Equal</Operator>
          <FilterValues>
            <FilterValue>=IIf(Parameters!pLabour_Account_Set_Count.Value = Parameters!pLabour_Account_Set.Count,Trim(Parameters!pResort_Code.Value),1)</FilterValue>
          </FilterValues>
        </Filter>
      </Filters>
    </DataSet>
